/*Lab QUE2:Write a program to calculate HCF of Two given number.*/
import java.util.Scanner;

public class Hcf {

    public static void main(String[] args) 
	{

		int a,b,hcf=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter two  number");
		a= sc.nextInt();
		b=sc.nextInt();
		
		for (int i = 1; i <= a || i <= b; i++)         //we use forn loop here
	      {
	     if (a % i == 0 && b % i == 0)          //check if both no divisible by same no then we get hcf for that we use &&
	         hcf = i;
	      }

	    System.out.println("HCF of Two "+a+ " and "+b+" is "+ hcf);
	     sc.close();
    }
    }
